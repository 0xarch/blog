<div'[widget cardArchives]>
    !a'[header].href(/archive/).in(
        !i'[fa-fw fas fa-archive].in();
        !span.in(归档);
        !span'[widgetHeaderLink].in(!i'[fas fa-angle-right].in(););
    );
    <div'[content]>
        !h3'[m0].in(最近的文章);
        <each:RecentPosts:item>
            !div'[listItem lp].in(!a.href(@item.src@).in(@item.title@););
        </each>
    </div>
</div>