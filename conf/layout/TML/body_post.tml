header'[shrinked]{
    div'[centerArea]{
        h1{@post.title@}
        h3{发布于 @post.date@}
    }
}
div'[wrapper]{
    div'[wrapperItem wrapperMain]{
        div'[widget nohover postContent]{
            @post.content@
        }
    }
    div'[wrapperItem wrapperRight]{
        list'[sticky wrapsFather]{
            include(widgets/about);
            include(widgets/toc);
            include(widgets/archivelist);
        }
    }
}